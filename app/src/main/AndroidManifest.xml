<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="alex.pakshin.trainingschedual" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TrainingSchedual" >
        <activity
            android:name=".DecriptionActivity"
            android:exported="false" />
        <activity
            android:name=".DescriptionActivity"
            android:exported="false"
            android:label="@string/title_activity_description"
            android:theme="@style/Theme.TrainingSchedual.NoActionBar" />
        <activity
            android:name=".NewWorkActivity"
            android:exported="false" />
        <activity
            android:name=".CalendarActivity"
            android:exported="true"
            android:label="@string/title_activity_calendar"
            android:theme="@style/Theme.TrainingSchedual.NoActionBar" />
        <activity
            android:name=".AddNewPlanActivity"
            android:exported="true" />
        <activity
            android:name=".AdminActivity"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ClientActivity"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SingUpActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>